{
  "name": "electron-react-client",
  "version": "1.1.0",
  "description": "A Electron desktop application. Built with Electron and React",
  "main": "main.js",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.config",
    "electron": "cross-env NODE_ENV=development electron.cmd .",
    "release": "webpack --config webpack.prod.config && cd dist && npm run inst && cd ../ && electron-packager dist chat-client --icon=app/resources/appIcon.ico --platform=win32 --electronVersion=2.0.7 --overwrite && rimraf -rf dist",
    "inst": "npm install cross-env && cross-env npm_config_target=2.0.7 npm_config_arch=x64 npm_config_target_arch=x64 npm_config_disturl=https://atom.io/download/electron npm_config_runtime=electron npm_config_build_from_source=true npm install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chenlizan/electron-react-client.git"
  },
  "keywords": [
    "electron",
    "react"
  ],
  "author": "chenlizan",
  "contributors": [
    {
      "name": "cicizss",
      "url": "https://github.com/cicizss"
    }
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chenlizan/electron-react-client/issues"
  },
  "homepage": "https://github.com/chenlizan/electron-react-client",
